<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.jianlu8023.multidbexample.web.db2.mapper.FileMapper">

    <resultMap id="BaseResultMap" type="com.github.jianlu8023.multidbexample.web.db2.entity.File">
            <id property="uid" column="uid" jdbcType="BIGINT"/>
            <result property="fileName" column="fileName" jdbcType="VARCHAR"/>
            <result property="fileDescribe" column="fileDescribe" jdbcType="VARCHAR"/>
            <result property="fileSize" column="fileSize" jdbcType="BIGINT"/>
            <result property="fileUploadTime" column="fileUploadTime" jdbcType="VARCHAR"/>
            <result property="fileType" column="fileType" jdbcType="VARCHAR"/>
            <result property="isArchive" column="isArchive" jdbcType="TINYINT"/>
            <result property="isDelete" column="isDelete" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        uid,fileName,fileDescribe,
        fileSize,fileUploadTime,fileType,
        isArchive,isDelete
    </sql>
</mapper>
